{
  "expo": {
    "name": "DTR",
    "slug": "DTR",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/logo.png",
  "scheme": "dtr",
    "userInterfaceStyle": "automatic",
  "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lovedev444.DTR"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
  "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fb8c3e6e-e19e-4c4f-a9e8-ac22931ccf6b"
      }
    }
  }
}
